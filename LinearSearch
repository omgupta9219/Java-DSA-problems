public class main{
    public static int linearSearch(int arr[], int target){
        if(arr.length==0){
            return -1;

        }
        for(int i=0;i<arr.length;i++){
            if(arr[i]==target){
                return i;
            }
        }

        return -1;
    }
    public static void main(String[] args) {
        int arr[]={1,2,3,4,5,28,29,19};
        int target=199;
        System.out.println(linearSearch(arr,target));
    }
}
